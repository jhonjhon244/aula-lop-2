#include <stdio.h>
int main() {
    char nome[50];
    int ganhos, gastos, saldo;
    printf("Digite o nome:");
    scanf( "%s", nome);


    printf("digite os ganhos:");
    scanf("%d", &ganhos);
    
    printf("digite os gastos");
    scanf("%d", &gastos);
    
    printf("digite o saldo");
    scanf("%d", &saldo);
    

    if (saldo>=200){
        
        printf("tenho saldo");

    }else{

        printf("pouco saldo");
}

return 0;
}
